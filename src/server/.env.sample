AZURE_OPENAI_API_KEY=key-goes-here # Required
AZURE_OPENAI_API_VERSION=2024-12-01-preview # Required
AZURE_OPENAI_ENDPOINT=https://example.openai.azure.com/ # Required
AZURE_OPENAI_DEPLOYMENT=gpt-4o # Required
AZURE_OPENAI_TRIAGE_MODEL=gpt-4o-mini # Required
AZURE_OPENAI_CARE_NAV_MODEL=gpt-4o # Required
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-small # Required

# AI Search Service Configuration
SEARCH_API_KEY=your-search-api-key # Optional
SEARCH_ENDPOINT=https://your-search-service.search.windows.net/ # Optional
SEARCH_INDEX_NAME=your-index-name # Optional

# Langfuse Configuration 
LANGFUSE_SECRET_KEY= # Optional
LANGFUSE_PUBLIC_KEY= # Optional
LANGFUSE_HOST= # Optional

# Azure Authentication (for Cosmos DB AAD/Entra auth)
AZURE_TENANT_ID=your-azure-tenant-id # Optional - for Service Principal auth
AZURE_CLIENT_ID=your-azure-client-id # Optional - for Service Principal auth
AZURE_CLIENT_SECRET=your-azure-client-secret # Optional - for Service Principal auth

# Cosmos DB Configuration
COSMOS_ENDPOINT=https://your-endpoint.documents.azure.com:443/ # Required
COSMOS_KEY=your-cosmos-key # Optional - for Primary Key auth (local emulator or dev)
COSMOS_DATABASE_NAME=dr_indigo_db # Required
COSMOS_CONTAINER_NAME=memory_container # Required
